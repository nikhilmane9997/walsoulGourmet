  jQuery.ajax({
    url: custom_ajax.ajaxurl,
    type: 'post',
    data: {
      action: 'load_post_content',
            postid : post_id
    },
    success: function( res ) {
             
      var result = JSON.parse(res);
           //console.log(result);
      		
