function ajaxpost(post_id){
  if(!s.hasClass('active')){
              s.addClass('active');
              jQuery('body').css('overflow-y','hidden');
            }
  
   
  jQuery('#elementor-popup-modal-30174 .elementor-element-populated .elementor-widget-wrap').hide();
