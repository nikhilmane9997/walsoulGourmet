            c.html(result.content);
            img.attr('src',result.img);
      		img.attr('alt',result.title);
          
            var fbl = 'http://www.facebook.com/sharer.php?u=' + result.url;
            var twl = 'https://twitter.com/intent/tweet?text=' + result.title + '&url=' + result.url + '&via=ENTRACKR';
            var lnl = 'https://www.linkedin.com/shareArticle?mini=true&url=' + result.url + '&summary=' + result.title;
      		var wal = 'https://wa.me/?text='+result.url;
            
            fb.attr('href',fbl);
            tw.attr('href',twl);
            ln.attr('href',lnl);
           	wa.attr('href',wal);
            fb.show();tw.show();ln.show();wa.show();
            prev_title.text(result.ptitle);
            next_title.text(result.ntitle);
          
            prev_url.attr('href',result.purl);
            prev_url.attr('data-id',result.pid);
            prev_btn.attr('data-id',result.pid);
            next_url.attr('href',result.nurl);
            next_url.attr('data-id',result.nid);
            next_btn.attr('data-id',result.nid);
          
          
            if(!result.pid){
              prev_url.hide();
              prev_btn.hide();
            }
            else{
              prev_url.show();
              prev_btn.show();
            }
            if(!result.nid){
              next_url.hide();
              next_btn.hide();
            }
            else{
                next_url.show();
                next_btn.show();
            }
            if ("ga" in window) {
              tracker = ga.getAll()[0];
              if (tracker) {
                  tracker.set("page", result.url);
